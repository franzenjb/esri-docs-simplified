# OpenAI API Configuration (Optional - for enhanced content processing)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude API (Optional - alternative to OpenAI)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Esri Configuration (Optional - for authenticated requests)
ARCGIS_USERNAME=your_arcgis_username
ARCGIS_PASSWORD=your_arcgis_password

# Processing Configuration
MAX_PAGES_TO_SCRAPE=10
PROCESSING_DELAY_MS=1000
MAX_RETRIES=3

# Output Configuration
OUTPUT_FORMAT=markdown
INCLUDE_IMAGES=true
INCLUDE_CODE_SAMPLES=true